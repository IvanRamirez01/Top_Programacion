package prueba;

import javax.swing.JOptionPane;

/**
 *
 * @author invitado
 */
public class Prueba {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        String numero1 = JOptionPane.showInputDialog("Ingrese un número: ");
        String numero2 = JOptionPane.showInputDialog("Ingrese otro número: ");
        //Convertir las entradas String en valores int para usarlos en el cálculo
        int primerNumero = Integer.parseInt(numero1);
        int segundoNumero = Integer.parseInt(numero2);
        
        //Declarar la variable
        int suma;
        
        //Realizamos suma
        suma = primerNumero + segundoNumero;
        
        //Mostrar los resultados en un diálogo de mensajes
        JOptionPane.showMessageDialog(null, "La suma es: " + suma);
        
    }
    
}
